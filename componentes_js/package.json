{
  "name": "usandoflex",
  "version": "1.0.0",
  "description": "usando flex",
  "main": "d.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\"  \"npm run dev\"  ",
    "scss": "node-sass  scss/ -o css/",
    "watch:scss": "onchange 'scss/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin precios.html -d dist --htmlmin -o dist/precios.html" ,
    "build":" npm run clean && npm run scss && npm run imagemin npm run usemin"


  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "gulp-imagemin": "^7.1.0",
    "jquery": "^3.5.1",
    "node-gyp": "^7.1.2",
    "open-iconic": "^1.1.1",
    "optipng-bin": "^7.0.0",
    "popper.js": "^1.16.1"
  }
}
